
  <h1>Songs</h1>

<div><%= link_to 'Create Song', new_song_path %></div>

<table >
  <tr>
    <th>Title</th>
    <th>Year</th> 
    <th>Artist name</th>
    <th>Genre</th>
    <th></th>
  </tr>





 <tbody>
<tr>
  <% @songs.each do |song|%>
  <td><%=song.title%> </td>
 
  <td><%=song.release_year%></td>
  <td><%=song.artist_name%></td>
  <td><%=song.genre%></td>

 <td>
   <%= link_to 'View', song %> |
  <%= link_to "Edit", edit_song_path(song) %> |
<%= link_to 'Delete', song, method: :delete,  data: { confirm: 'Are you sure?' } %>
</td>
   </tr>
<%end%>
 </tbody>
 </table>


