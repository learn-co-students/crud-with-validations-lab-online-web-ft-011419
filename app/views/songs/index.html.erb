<h1>Songs</h1>
<% @songs.each do |song| %>
<%= song.title %>
<%= song.release_year %>
<%= song.released %>
<%= song.artist_name %>
<%= song.genre %>
<%= link_to 'Show', song %>
<%= link_to 'Edit', edit_song_path(song) %>
<%= link_to 'Destroy', song, method: :delete, data: {confirm: 'Are you sure?'}%>
<% end %>
