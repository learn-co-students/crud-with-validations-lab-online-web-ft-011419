<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Artist</th>
      <th>Released</th>
      <th>Release Year</th>
    </tr>
  </thead>
  <%= @songs.each do |song| %>
  <tbody>
    <tr>
      <td><%= link_to song.title, song_path(song) %><td>
      <td><%= link_to song.artist_name %><td>
      <td><%= link_to song.released %><td>
      <td><%= link_to song.release_year %><td>
    </tr>
  </tbody>
  <% end %>
</table>

 <div><%= link_to 'Create a Song', new_song_path %></div>
